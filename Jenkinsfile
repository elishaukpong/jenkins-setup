        echo 'Building'
      }
    }

    stage('Testing') {
      steps {
        echo 'Testing'
      }
    }

    stage('Deploying') {
      steps {
        echo 'Deploying'
      }
    }

    stage('Over') {
      steps {
        echo 'Hello'
      }
    }

  }
}